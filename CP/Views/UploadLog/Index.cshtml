@{
   ViewBag.Title = "Upload Log";
   int SrNo = 1;
}
<div class="row">
    <div class="col-md-12">
        <div class="panel">
            <div class="panel-heading">
                <br />
                <h3 class="panel-title">Upload Log</h3>
                <br />
            </div>
            <div class="panel-body">

                <table class="table table-striped table-bordered" id="UploadLog">

                    <thead>
                        <tr>
                            <th><a href="#">Sr No.</a></th>
                            <th><a href="#">Company</a></th>
                            <th><a href="#">FY@((ViewBag.FinYear as List<int>)[0].ToString().Substring(2))</a></th>
                            <th><a href="#">FY@((ViewBag.FinYear as List<int>)[1].ToString().Substring(2))</a></th>
                            <th><a href="#">FY@((ViewBag.FinYear as List<int>)[2].ToString().Substring(2))</a></th>
                            <th><a href="#">FY@((ViewBag.FinYear as List<int>)[3].ToString().Substring(2))</a></th>
                            <th><a href="#">Uploaded By</a></th>
                            <th><a href="#">Uploaded On</a></th>

                        </tr>
                    </thead>
                    <tbody>
                        @if (ViewBag.UploadLog != null && ViewBag.UploadLog.Count > 0)
                        {

                            foreach (var viewmodel in ViewBag.UploadLog)
                            {
                                <tr>

                                    <td>@(SrNo++)</td>
                                    <td>@viewmodel.CompanyName</td>

                                    <td><input type="checkbox" @(viewmodel.FinYear1 > 0 ? "checked" : "") disabled="disabled"></td>
                                    <td><input type="checkbox" @(viewmodel.FinYear2 > 0 ? "checked" : "") disabled="disabled"></td>
                                    <td><input type="checkbox" @(viewmodel.FinYear3 > 0 ? "checked" : "") disabled="disabled"></td>
                                    <td><input type="checkbox" @(viewmodel.FinYear4 > 0 ? "checked" : "") disabled="disabled"></td>
                                    <td>@viewmodel.LastUploadedBy</td>
                                    <td>@String.Format("{0:dd-MMM-yyyy | hh:mm:ss tt }", viewmodel.LastUploadedOn)</td>
                                </tr>
                            }
                        }
                        else
                        {
                            <tr class="text-center"><td colspan="8">No Records Found</td></tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
